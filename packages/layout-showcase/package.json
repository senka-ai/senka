  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "6.0.1",
    "@sveltejs/kit": "2.27.1",
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@tailwindcss/cli": "4.1.12",
    "remixicon": "^4.6.0",
    "svelte": "5.37.3",
    "svelte-bootstrap-svg-icons": "^2.0.0",
    "svelte-check": "4.3.1",
    "svelte-heros": "^7.0.2",
    "svelte-remix": "^2.0.1",
    "tailwindcss": "4.1.12",
    "typescript": "5.9.2",
    "vite": "7.0.6"
  },
  "type": "module"
